{
  "name": "pristine-bot",
  "version": "1.0.0",
  "description": "",
  "main": "./builds/dist/index.js",
  "scripts": {
    "start": "concurrently \"npm run parcel-bot\" \"npm run bot\"",
    "start:db": "concurrently \"npm run parcel-db\" \"npm run db\"",
    "boot": "npm run build && node builds/dist/index",
    "build": "parcel build ./src/index.ts --target=node -d builds/dist",
    "bot": "nodemon builds/bot/index.js --inspect --trace-warnings --config nodemon.json",
    "db": "nodemon builds/db/db.js --inspect --trace-warnings --config nodemon.json",
    "parcel-db": "parcel ./src/database/db.ts --log-level 3 --target=node -d builds/db/",
    "parcel-bot": "parcel ./src/index.ts --log-level 3 --target=node -d builds/bot/"
  },
  "author": "Wallace Fares",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.16.0",
    "@types/sequelize": "^4.27.28",
    "axios": "^0.18.0",
    "better-log": "^1.3.3",
    "cleverbot.io": "^1.0.4",
    "concurrently": "^4.0.1",
    "discord.js": "^11.4.2",
    "express": "^4.16.4",
    "google-translate-api": "^2.3.0",
    "jsdom": "^12.2.0",
    "mysql2": "^1.6.1",
    "nodemon": "^1.18.4",
    "sequelize": "^4.39.1",
    "wordnet": "^0.1.2"
  },
  "devDependencies": {
    "parcel-bundler": "^1.10.3",
    "tslib": "^1.9.3",
    "typescript": "^3.1.3"
  }
}

{
  "name": "pristine-bot",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "start": "concurrently \"npm run parcel-bot\" \"npm run bot\"",
    "boot": "npm run build && node dist/index",
    "build": "parcel build src/index.ts --target=node -d builds/dist",
    "bot": "nodemon builds/bot/index --inspect --trace-warnings --config nodemon.json",
    "db": "nodemon builds/db/db --inspect --trace-warnings --config nodemon.json",
    "parcel-db": "parcel src/database/db.ts --log-level 3 --target=node -d db/",
    "parcel-bot": "parcel src/index.ts --log-level 3 --target=node -d bot/"
  },
  "author": "Wallace Fares",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.16.0",
    "@types/sequelize": "^4.27.23",
    "axios": "^0.18.0",
    "better-log": "^1.3.3",
    "cleverbot.io": "^1.0.4",
    "concurrently": "^3.5.1",
    "discord.js": "^11.3.2",
    "express": "^4.16.3",
    "google-translate-api": "^2.3.0",
    "jsdom": "^11.11.0",
    "mysql2": "^1.5.3",
    "nodemon": "^1.17.5",
    "sequelize": "^4.38.0",
    "wordnet": "^0.1.2"
  },
  "devDependencies": {
    "parcel-bundler": "^1.9.7",
    "tslib": "^1.9.2",
    "typescript": "^2.9.1"
  }
}
